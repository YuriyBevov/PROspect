-
  const navList = [
    { link: '#portfolio', title: 'Портфолио' }, { link: '#features', title: 'Услуги' },
    { link: '#advertising ', title: 'Наружная реклама' }, { link: '#feedback', title: 'Обратная связь' },
    { link: '#map', title: 'Как нас найти' },
  ]

nav.nav
  .nav__underline(aria-hidden="true")
  ul.nav__list
    each item in navList
      li.nav__item
        a(href=item.link).nav__link= item.title
